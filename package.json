{
  "name": "vue-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run tests/unit/",
    "test:integration": "vitest --run tests/integration/",
    "test:e2e": "vitest -c vitest.e2e.config.ts --run tests/e2e/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "type-check": "vue-tsc --noEmit --project tsconfig.app.json",
    "lint": "eslint src --fix",
    "format": "prettier src --list-different --write"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^13.9.0",
    "daisyui": "^5.1.31",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.14",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.7.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vitest-browser-vue": "^1.1.0",
    "vue-tsc": "^2.2.8"
  }
}